{
  "consumer": {
    "name": "AdminPortal"
  },
  "interactions": [
    {
      "description": "a request for the administrative product listing",
      "pending": false,
      "providerStates": [
        {
          "name": "products exist"
        }
      ],
      "request": {
        "headers": {
          "Accept": [
            "application/json"
          ]
        },
        "method": "GET",
        "path": "/products"
      },
      "response": {
        "body": {
          "content": [
            {
              "color": 4,
              "description": "Freshly brewed coffee every morning",
              "id": 1,
              "inStock": true,
              "name": "Coffee Machine",
              "price": 199.99,
              "slug": "coffee_machine_110"
            },
            {
              "color": 4,
              "description": "Freshly brewed coffee every morning",
              "id": 1,
              "inStock": true,
              "name": "Coffee Machine",
              "price": 199.99,
              "slug": "coffee_machine_110"
            }
          ],
          "contentType": "application/json",
          "encoded": false
        },
        "headers": {
          "Content-Type": [
            "application/json; charset=utf-8"
          ]
        },
        "matchingRules": {
          "body": {
            "$": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type",
                  "min": 2
                }
              ]
            },
            "$[*].color": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "integer"
                },
                {
                  "match": "integer"
                }
              ]
            },
            "$[*].description": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                },
                {
                  "match": "type"
                }
              ]
            },
            "$[*].id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "integer"
                },
                {
                  "match": "integer"
                }
              ]
            },
            "$[*].inStock": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                },
                {
                  "match": "type"
                }
              ]
            },
            "$[*].name": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                },
                {
                  "match": "type"
                }
              ]
            },
            "$[*].price": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "decimal"
                },
                {
                  "match": "decimal"
                }
              ]
            },
            "$[*].slug": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                },
                {
                  "match": "type"
                }
              ]
            }
          }
        },
        "status": 200
      },
      "type": "Synchronous/HTTP"
    },
    {
      "description": "an administrative request to edit a missing product",
      "pending": false,
      "providerStates": [
        {
          "name": "product with ID 999 does not exist"
        }
      ],
      "request": {
        "headers": {
          "Accept": [
            "application/json"
          ]
        },
        "method": "GET",
        "path": "/products/999"
      },
      "response": {
        "body": {
          "content": {
            "message": "Product not found"
          },
          "contentType": "application/json",
          "encoded": false
        },
        "headers": {
          "Content-Type": [
            "application/json; charset=utf-8"
          ]
        },
        "matchingRules": {
          "body": {
            "$": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          }
        },
        "status": 404
      },
      "type": "Synchronous/HTTP"
    },
    {
      "description": "an administrative request to edit a product",
      "pending": false,
      "providerStates": [
        {
          "name": "product with ID 2 exists"
        }
      ],
      "request": {
        "headers": {
          "Accept": [
            "application/json"
          ]
        },
        "method": "GET",
